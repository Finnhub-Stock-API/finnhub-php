{
    "name": "finnhub/client",
    "version": "1.1.0",
    "description": "Finnhub PHP api client",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://finnhub.io/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Finnhub",
            "homepage": "https://finnhub.io/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Finnhub\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Finnhub\\" : "test/" }
    }
}
